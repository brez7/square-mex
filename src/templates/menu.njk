<section class="py-13 py-lg-15">
  <div
    class="bg-image bg-cover overlay overlay-primary overlay-50"
    data-jarallax
    data-speed=".8"
    style="
      background-image: url(assets/images/photos/breakfast/breakfast-main.webp);
    "
  >
    <div class="container h-100">
      <div
        class="row align-items-center justify-content-center h-100 text-center text-white"
      >
        <div class="col-lg-6 position-relative">
          <h1 class="display-4">El Pueblo Mexican <br />Food Menu</h1>
        </div>
      </div>
    </div>
  </div>
</section>

